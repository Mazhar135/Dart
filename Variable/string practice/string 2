void main ()
{
  String age = 'HelloWorld';
  String index = '01234     56789';
  String result;

  result = age + index;

  print(age.length);   // this is how to get the lengthof string
  print(index.length); 

  print(age[5]);  // this is how to we get the any value of inside the string
  print(result);
}