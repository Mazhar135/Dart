class Person{
  String _name;
  int _age;


  // Constructor
  Person(this._name, this._age);

  String get name => _name;
  int get age => _age;

  set setAge(int age)
  {
if(age> 0)
{
  _age = age;
}
  }


  //Method to  display person detials
  void displayInfo()
  {
    print('Name: $_name, Age; $_age');
  }
}