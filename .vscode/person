class Person{
  String name;
  int age;


  // Constructor
  Person(this.name, this.age);

  String get name => _name;
  int get age => _age;

  set setAge(int age)
  {
if(age> 0)
{
  _age = age;
}
  }


  //Method to  display person detials
  void displayInfo()
  {
    print('Name: $name, Age; $age');
  }
}